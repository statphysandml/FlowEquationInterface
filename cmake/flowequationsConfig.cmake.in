@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

# Find devdat as a dependency
find_dependency(devdat REQUIRED)

# Handle nlohmann_json dependency - only if we didn't export our own wrapper
if(@FLOWEQUATIONS_USE_SYSTEM_JSON@ AND NOT @EXPORT_JSON_TARGET@)
    find_dependency(nlohmann_json REQUIRED)
endif()

# Include the exported targets
include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Targets.cmake")

# Provide compatibility variables
set(flowequations_FOUND TRUE)
set(flowequations_VERSION @PROJECT_VERSION@)
set(flowequations_INCLUDE_DIRS "${CMAKE_CURRENT_LIST_DIR}/../../../include")

message(STATUS "Found @PROJECT_NAME@ @PROJECT_VERSION@")